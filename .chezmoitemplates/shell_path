# this should be run before the other things in shell/

for p in \
  /usr/sbin \
  /sbin \
  /usr/X11/bin \
  /usr/local/sbin \
  /usr/local/bin \
  /opt/local/sbin \
  /opt/local/bin \
  /usr/local/opt/coreutils/libexec/gnubin \
  /usr/local/opt/tcl-tk/bin \
  $HOME/.bin \
  $HOME/.local/bin \

do
  export PATH="$p:$PATH"
done
